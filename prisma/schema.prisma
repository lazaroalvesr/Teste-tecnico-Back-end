generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://watersuply:suplywater@localhost:5432/mywater"
}

model Customer {
  id            String    @id @default(uuid())
  customer_code String    @unique
  Reading       Reading[]
}

model Reading {
  id               String   @id @default(uuid())
  measure_uuid     String   @unique @default(uuid())
  image_url        String
  measure_value    Int
  has_confirmed    Boolean
  measure_datetime DateTime
  measure_type     String
  customer_Code    Customer @relation(fields: [customerId], references: [customer_code])
  customerId       String
}
